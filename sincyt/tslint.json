{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": [
            false
        ],
        "triple-equals": [
            false
        ],
        "no-empty": [
            false
        ],
        "variable-name": [
            false
        ],
        "no-shadowed-variable": [
            false
        ],
        "only-arrow-functions": [
            false
        ],
        "class-name": [
            false
        ],
        "prefer-for-of": [
            false
        ]
    },
    "rulesDirectory": []
}